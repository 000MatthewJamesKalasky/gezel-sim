all:
	cd ./gezel;     make ; cd .
	cd ./systemc;   make ; cd .
	cd ./java;      make ; cd .
	cd ./gplatform; make ; cd .

sim:
	cd ./gezel;     make sim; cd .
	cd ./systemc;   make sim; cd .
	cd ./java;      make sim; cd .
	cd ./gplatform; make sim; cd .

clean:
	cd ./gezel;     make clean; cd .
	cd ./systemc;   make clean; cd .
	cd ./java;      make clean; cd .
	cd ./gplatform; make clean; cd .
